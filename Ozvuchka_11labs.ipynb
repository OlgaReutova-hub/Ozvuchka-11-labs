        "import telebot\n",
        "from telebot import types\n",
        "from elevenlabs.client import ElevenLabs\n",
        "import key\n",
        "\n",
        "# -------------------------------\n",
        "# üîß –ù–∞—Å—Ç—Ä–æ–π–∫–∏\n",
        "# -------------------------------\n",
        "bot = telebot.TeleBot(key.bot_token)\n",
        "client = ElevenLabs(api_key=key.elevenlabs_api_key)\n",
        "\n",
        "# -------------------------------\n",
        "# üéß –§—É–Ω–∫—Ü–∏—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —Ä–µ—á–∏\n",
        "# -------------------------------\n",
        "def tts_generate(text: str, voice_id: str, model_id: str = \"eleven_multilingual_v2\", filename: str = \"output.mp3\"):\n",
        "    \"\"\"–ì–µ–Ω–µ—Ä–∏—Ä—É–µ—Ç –∞—É–¥–∏–æ –∏–∑ —Ç–µ–∫—Å—Ç–∞ –∏ —Å–æ—Ö—Ä–∞–Ω—è–µ—Ç –≤ —Ñ–∞–π–ª\"\"\"\n",
        "    audio_stream = client.text_to_speech.convert(\n",
        "        voice_id=voice_id,\n",
        "        model_id=model_id,\n",
        "        text=text,\n",
        "        output_format=\"mp3_44100_128\"\n",
        "    )\n",
        "\n",
        "    with open(filename, \"wb\") as f:\n",
        "        for chunk in audio_stream:\n",
        "            f.write(chunk)\n",
        "\n",
        "    return filename\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# üéô –ó–∞–≥—Ä—É–∑–∫–∞ –≥–æ–ª–æ—Å–æ–≤\n",
        "# -------------------------------\n",
        "voices_resp = client.voices.get_all()\n",
        "voices_list = voices_resp.voices\n",
        "\n",
        "male_voices = [v for v in voices_list if \"male\" in (v.labels or {}).get(\"gender\", \"\").lower()]\n",
        "female_voices = [v for v in voices_list if \"female\" in (v.labels or {}).get(\"gender\", \"\").lower()]\n",
        "\n",
        "if not male_voices:\n",
        "    male_voices = [v for v in voices_list if \"man\" in v.name.lower()]\n",
        "if not female_voices:\n",
        "    female_voices = [v for v in voices_list if \"woman\" in v.name.lower()]\n",
        "\n",
        "user_gender = {}\n",
        "user_voice_choice = {}\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# üöÄ –ö–æ–º–∞–Ω–¥–∞ /start\n",
        "# -------------------------------\n",
        "@bot.message_handler(commands=['start'])\n",
        "def start(message):\n",
        "    markup = types.InlineKeyboardMarkup()\n",
        "    markup.add(types.InlineKeyboardButton(\"üßë‚Äçüíº –ú—É–∂—Å–∫–æ–π –≥–æ–ª–æ—Å\", callback_data=\"gender_male\"))\n",
        "    markup.add(types.InlineKeyboardButton(\"üë© –ñ–µ–Ω—Å–∫–∏–π –≥–æ–ª–æ—Å\", callback_data=\"gender_female\"))\n",
        "    bot.send_message(\n",
        "        message.chat.id,\n",
        "        \"üéô –ü—Ä–∏–≤–µ—Ç! –Ø –±–æ—Ç, –∫–æ—Ç–æ—Ä—ã–π –æ–∑–≤—É—á–∏–≤–∞–µ—Ç —Ç–µ–∫—Å—Ç. –°–Ω–∞—á–∞–ª–∞ –≤—ã–±–µ—Ä–∏ —Ç–∏–ø –≥–æ–ª–æ—Å–∞:\",\n",
        "        reply_markup=markup\n",
        "    )\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# üß© –í—ã–±–æ—Ä –ø–æ–ª–∞\n",
        "# -------------------------------\n",
        "@bot.callback_query_handler(func=lambda call: call.data in [\"gender_male\", \"gender_female\"])\n",
        "def choose_gender(call):\n",
        "    user_id = call.from_user.id\n",
        "    gender = \"male\" if call.data == \"gender_male\" else \"female\"\n",
        "    user_gender[user_id] = gender\n",
        "\n",
        "    voices_list_by_gender = male_voices if gender == \"male\" else female_voices\n",
        "\n",
        "    markup = types.InlineKeyboardMarkup()\n",
        "    for v in voices_list_by_gender[:6]:\n",
        "        markup.add(types.InlineKeyboardButton(v.name, callback_data=f\"preview_{v.voice_id}\"))\n",
        "\n",
        "    bot.send_message(\n",
        "        call.message.chat.id,\n",
        "        f\"üéß –û—Ç–ª–∏—á–Ω–æ! –¢—ã –≤—ã–±—Ä–∞–ª {'–º—É–∂—Å–∫–∏–µ' if gender=='male' else '–∂–µ–Ω—Å–∫–∏–µ'} –≥–æ–ª–æ—Å–∞.\\n\"\n",
        "        \"–ù–∞–∂–º–∏ –Ω–∞ –∏–º—è, —á—Ç–æ–±—ã –ø—Ä–æ—Å–ª—É—à–∞—Ç—å –ø—Ä–∏–º–µ—Ä:\",\n",
        "        reply_markup=markup\n",
        "    )\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# ‚ñ∂Ô∏è –ü—Ä–æ—Å–ª—É—à–∏–≤–∞–Ω–∏–µ –ø—Ä–∏–º–µ—Ä–æ–≤\n",
        "# -------------------------------\n",
        "@bot.callback_query_handler(func=lambda call: call.data.startswith(\"preview_\"))\n",
        "def preview_voice(call):\n",
        "    voice_id = call.data.split(\"_\", 1)[1]\n",
        "    voice = next((v for v in voices_list if v.voice_id == voice_id), None)\n",
        "    if not voice:\n",
        "        bot.send_message(call.message.chat.id, \"‚ö†Ô∏è –ì–æ–ª–æ—Å –Ω–µ –Ω–∞–π–¥–µ–Ω.\")\n",
        "        return\n",
        "\n",
        "    bot.send_message(call.message.chat.id, f\"üîä –ü—Ä–∏–º–µ—Ä –≥–æ–ª–æ—Å–∞ *{voice.name}*:\", parse_mode=\"Markdown\")\n",
        "\n",
        "    try:\n",
        "        filename = tts_generate(\"–ü—Ä–∏–≤–µ—Ç! –≠—Ç–æ –ø—Ä–∏–º–µ—Ä –º–æ–µ–≥–æ –≥–æ–ª–æ—Å–∞.\", voice_id, filename=\"sample.mp3\")\n",
        "        with open(filename, \"rb\") as f:\n",
        "            bot.send_audio(call.message.chat.id, f, title=f\"–ì–æ–ª–æ—Å {voice.name}\")\n",
        "\n",
        "        markup = types.InlineKeyboardMarkup()\n",
        "        markup.add(types.InlineKeyboardButton(f\"‚úÖ –í—ã–±—Ä–∞—Ç—å {voice.name}\", callback_data=f\"select_{voice_id}\"))\n",
        "        bot.send_message(call.message.chat.id, \"–•–æ—á–µ—à—å –≤—ã–±—Ä–∞—Ç—å —ç—Ç–æ—Ç –≥–æ–ª–æ—Å?\", reply_markup=markup)\n",
        "    except Exception as e:\n",
        "        bot.send_message(call.message.chat.id, f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ–∑–¥–∞–Ω–∏–∏ –ø—Ä–∏–º–µ—Ä–∞: {e}\")\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# ‚úÖ –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ –≤—ã–±–æ—Ä–∞ –≥–æ–ª–æ—Å–∞\n",
        "# -------------------------------\n",
        "@bot.callback_query_handler(func=lambda call: call.data.startswith(\"select_\"))\n",
        "def select_voice(call):\n",
        "    voice_id = call.data.split(\"_\", 1)[1]\n",
        "    user_voice_choice[call.from_user.id] = voice_id\n",
        "    voice = next((v for v in voices_list if v.voice_id == voice_id), None)\n",
        "    bot.send_message(\n",
        "        call.message.chat.id,\n",
        "        f\"‚úÖ –ì–æ–ª–æ—Å *{voice.name}* –≤—ã–±—Ä–∞–Ω!\\n–¢–µ–ø–µ—Ä—å –æ—Ç–ø—Ä–∞–≤—å —Ç–µ–∫—Å—Ç, –∫–æ—Ç–æ—Ä—ã–π –Ω—É–∂–Ω–æ –æ–∑–≤—É—á–∏—Ç—å:\",\n",
        "        parse_mode=\"Markdown\"\n",
        "    )\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# üó£ –û–∑–≤—É—á–∏–≤–∞–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞\n",
        "# -------------------------------\n",
        "@bot.message_handler(func=lambda message: True)\n",
        "def handle_text(message):\n",
        "    user_id = message.from_user.id\n",
        "\n",
        "    if user_id not in user_voice_choice:\n",
        "        bot.reply_to(message, \"–°–Ω–∞—á–∞–ª–∞ –≤—ã–±–µ—Ä–∏ –≥–æ–ª–æ—Å ‚Äî –æ—Ç–ø—Ä–∞–≤—å /start\")\n",
        "        return\n",
        "\n",
        "    voice_id = user_voice_choice[user_id]\n",
        "    text = message.text.strip()\n",
        "\n",
        "    if not text:\n",
        "        bot.reply_to(message, \"–û—Ç–ø—Ä–∞–≤—å —Ç–µ–∫—Å—Ç –¥–ª—è –æ–∑–≤—É—á–∫–∏.\")\n",
        "        return\n",
        "\n",
        "    bot.send_message(message.chat.id, \"ü™Ñ –°–æ–∑–¥–∞—é –æ–∑–≤—É—á–∫—É... –ü–æ–¥–æ–∂–¥–∏ –Ω–µ–º–Ω–æ–≥–æ.\")\n",
        "\n",
        "    try:\n",
        "        filename = tts_generate(text, voice_id, filename=\"voice.mp3\")\n",
        "\n",
        "        with open(filename, \"rb\") as audio_file:\n",
        "            bot.send_audio(message.chat.id, audio_file, title=\"–û–∑–≤—É—á–∫–∞\")\n",
        "\n",
        "        with open(filename, \"rb\") as voice_file:\n",
        "            bot.send_voice(message.chat.id, voice_file)\n",
        "\n",
        "    except Exception as e:\n",
        "        bot.send_message(message.chat.id, f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ–∑–≤—É—á–∫–µ: {e}\")\n",
        "\n",
        "\n",
        "# -------------------------------\n",
        "# üöÄ –ó–∞–ø—É—Å–∫\n",
        "# -------------------------------\n",
        "print(\"‚úÖ –ë–æ—Ç –∑–∞–ø—É—â–µ–Ω –∏ –≥–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ!\")\n",
        "bot.infinity_polling()\n"
      ]
    }
